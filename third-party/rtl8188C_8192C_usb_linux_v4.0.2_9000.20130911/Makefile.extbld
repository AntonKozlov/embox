
PKG_NAME := rtlwifi
PKG_VER  := 4.0.2_9000.20130911

PKG_SOURCES :=
PKG_MD5     :=

PKG_PATCHES :=

include $(EXTBLD_LIB)

KERNELRELEASE=embox
CONFIG_RTL8192CU=y
include Makefile

objs := $($(MODULE_NAME)-y)

$(CONFIGURE) :
	touch $@

#XXX
$(objs) : CPPFLAGS=-DCONFIG_LITTLE_ENDIAN

$(objs) : %.o : %.c
	mkdir -p $(shell dirname $(EMBOX_ROOT_DIR)/build/base/obj/third-party/rtl8188C_8192C_usb_linux_v4.0.2_9000.20130911/$@); $(EMBOX_GCC) $(CPPFLAGS) -I./include -c -o ../../build/base/obj/third-party/rtl8188C_8192C_usb_linux_v4.0.2_9000.20130911/$@ $<

$(MODULE_NAME).o : $(objs)

$(BUILD) : $(MODULE_NAME).o
	touch $@

$(INSTALL) :
	touch $@

